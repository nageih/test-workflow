	quest.2465C83C97A189EA.quest_subtitle: "Best Buds"
	quest.2465C83C97A189EA.title: "&fPath of Crystalmancy"
	quest.2471364BFF963F98.title: "&fBow Blueprint"
	quest.2474908186EBBD0D.quest_desc: [
